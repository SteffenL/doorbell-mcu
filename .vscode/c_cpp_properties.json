{
  "configurations": [
    {
      "name": "ESP-IDF",
      "compilerPath": "${HOME}/.espressif/tools/xtensa-esp32-elf/esp-2020r3-8.4.0/xtensa-esp32-elf/bin/xtensa-esp32-elf-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "includePath": [
        "${config:idf.espIdfPath}/components/**",
        "${config:idf.espIdfPathWin}/components/**",
        "${config:idf.espAdfPath}/components/**",
        "${config:idf.espAdfPathWin}/components/**",
        "${workspaceFolder}/**",
        "${config:idf.espIdfPath}/components/driver/esp32/include",
        "${config:idf.espIdfPath}/components/driver/include",
        "${config:idf.espIdfPath}/components/esp_common/include",
        "${config:idf.espIdfPath}/components/esp_eth/include",
        "${config:idf.espIdfPath}/components/esp_event/include",
        "${config:idf.espIdfPath}/components/esp_http_client/include",
        "${config:idf.espIdfPath}/components/esp_netif/include",
        "${config:idf.espIdfPath}/components/esp_rom/include",
        "${config:idf.espIdfPath}/components/esp_system/include",
        "${config:idf.espIdfPath}/components/esp_timer/include",
        "${config:idf.espIdfPath}/components/esp_wifi/include",
        "${config:idf.espIdfPath}/components/esp32/include",
        "${config:idf.espIdfPath}/components/freertos/include",
        "${config:idf.espIdfPath}/components/freertos/xtensa/include",
        "${config:idf.espIdfPath}/components/heap/include",
        "${config:idf.espIdfPath}/components/lwip/include/apps/dhcpserver",
        "${config:idf.espIdfPath}/components/lwip/include/apps/sntp",
        "${config:idf.espIdfPath}/components/lwip/lwip/src/include",
        "${config:idf.espIdfPath}/components/lwip/port/esp32/include",
        "${config:idf.espIdfPath}/components/newlib/platform_include",
        "${config:idf.espIdfPath}/components/nghttp/port/include",
        "${config:idf.espIdfPath}/components/nvs_flash/include",
        "${config:idf.espIdfPath}/components/soc/include",
        "${config:idf.espIdfPath}/components/soc/soc/esp32/include",
        "${config:idf.espIdfPath}/components/soc/soc/include",
        "${config:idf.espIdfPath}/components/soc/src/esp32/include",
        "${config:idf.espIdfPath}/components/spi_flash/include",
        "${config:idf.espIdfPath}/components/tcpip_adapter/include",
        "${config:idf.espIdfPath}/components/xtensa/include",
        "${config:idf.espIdfPath}/components/xtensa/esp32/include",
        "${workspaceFolder}/build/config"
      ],
      "browse": {
        "path": [
          "${config:idf.espIdfPath}/components",
          "${config:idf.espIdfPathWin}/components",
          "${config:idf.espAdfPath}/components/**",
          "${config:idf.espAdfPathWin}/components/**",
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": false
      }
    }
  ],
  "version": 4
}
